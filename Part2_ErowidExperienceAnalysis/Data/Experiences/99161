</script>
<script>
var renderVSAMSGTSearch = function() {
	for(var a = 0; a < vsamsgtsearches.length; a++)     
		google.search.cse.element.render({div: vsamsgtsearches[a], tag: 'searchbox-only'})
}, startVSAMSGTSearch = function() {
	'complete' == document.readyState ? renderVSAMSGTSearch() : google.setOnLoadCallback(renderVSAMSGTSearch)
};	
window.__gcse = { parsetags: 'explicit', callback: startVSAMSGTSearch}, function() {
	var cx = 'partner-pub-7865546952023728:8665348048';
	var gcse = document.createElement('script');
	gcse.type = 'text/javascript';
	gcse.async = true;
	gcse.src = 'https://cse.google.com/cse.js?cx=' + cx;
	var s = document.getElementsByTagName('script')[0];
	s.parentNode.insertBefore(gcse, s);
}();
</script></body></html>
